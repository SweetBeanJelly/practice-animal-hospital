<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.jyk.administrator.animal.Reservation_subject"
    android:background="@drawable/subject">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/title"
        android:text="진료과목"
        android:textStyle="bold"
        android:textSize="18dp"
        android:gravity="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#252525"
        android:textIsSelectable="false"
        android:layout_marginTop="80dp" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="23dp"
        android:id="@+id/linearLayout3"
        android:gravity="center">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="외과 (09:00~20:00)"
            android:id="@+id/txt1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:background="#b3ffffff"
            android:width="200dp"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_marginTop="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="산과 (12:00~15:00)"
            android:id="@+id/txt2"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:gravity="center"
            android:paddingRight="25dp"
            android:paddingLeft="25dp"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="진단병리과 (10:00~20:00)"
            android:id="@+id/txt3"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:gravity="center"
            android:background="#b3ffffff"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:width="200dp"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="임상병리과 (10:00~18:00)"
            android:id="@+id/txt4"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:gravity="center"
            android:paddingRight="25dp"
            android:paddingLeft="25dp"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:layout_marginTop="5dp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="영상의학과 (10:00~18:00)"
            android:id="@+id/txt5"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:gravity="center"
            android:background="#b3ffffff"
            android:paddingLeft="8dp"
            android:paddingRight="8dp"
            android:width="200dp"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="안과 (09:00~20:00)"
            android:id="@+id/txt6"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:gravity="center"
            android:paddingRight="25dp"
            android:paddingLeft="25dp"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="피부과 (10:00~20:00)"
            android:id="@+id/txt7"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:gravity="center"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="내과 (09:00~20:00)"
            android:id="@+id/txt8"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:gravity="center"
            android:paddingRight="25dp"
            android:paddingLeft="25dp"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="응급진료과 (09:00~20:00)"
            android:id="@+id/txt9"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:gravity="center"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="야생동물질병과 (12:00~20:00)"
            android:id="@+id/txt10"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:gravity="center"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:width="200dp"
            android:background="#b3ffffff"
            android:textColor="#404040"
            android:textSize="12dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />
    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:text="닫기"
        android:id="@+id/btnE"
        android:gravity="center"
        android:layout_below="@+id/linearLayout3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:background="#ff8080"
        android:textColor="#ffffff"
        android:textStyle="bold" />


</RelativeLayout>
